#!c:/perl/bin/perl.exe
#Calculator.pm

package calc;

use strict;
use warnings;
use Exporter;

our @ISA = qw(Exporter);
our @EXPORT = qw(add);

sub add
{
	my ($num1,$num2) = @_;
    $num1+$num2;
}

1;

