library(pls)
quality.data <- read.table(textConnection(Lines), header = TRUE)
pairs(quality.data[c("x4", "x5", "y1")])
quality.simpls <- mvr(y1 ~., ncomp = 6, quality.data, method = "simpls",
  validation = "CV") # replace 6 with whatever you want



