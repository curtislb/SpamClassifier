#!/usr/bin/perl
use strict;
   
  my @Strings = qw(lmn lpt lor mps npr nqt opq rst);
   
  chomp(my $Input = <STDIN>);

  my $SortedChars = join "", sort (split //, $Input);
  foreach (@Strings)
{
        if ($SortedChars =~ /$_/)
        {
                print "Got a match for $Input with $_\n";
        }
}

