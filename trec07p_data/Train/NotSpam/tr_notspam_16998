## Loading Data
library(MASS)
olive <- url("
http://www.statistik.tuwien.ac.at/public/filz/students/multi/ss07/olive.R")
print(load(olive))
dim(olive)
summary(olive)

index <- sample(nrow(olive), 286)

train  <- olive[index,-11]
test   <- olive[-index,-11]

summary(train)
summary(test)

table(train$Region)
table(test$Region)

# Linear Discriminant Analysis
z <- lda(Region ~ . , train)
zn <- predict(z, newdata=test)$class
mean(zn != test$Region)

